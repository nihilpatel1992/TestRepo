@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <div>

        <table border='0' cellspacing='0' align='center' style='width:750px; margin:0 auto;'>
            <tr> <td colspan='2' valign='middle' align='center' style='font-size: 13px;font-weight: bold'><img style='margin-top:5px;padding-bottom:4px;width:150px;' src='https://wotu.azureedge.net/web/wotu-l-logo.jpg' /> </td></tr>
            <tr> <td colspan='2' valign='middle' align='center' style='font-size: 13px;font-weight: bold;width:100%;padding-top:4px;padding-bottom:4px;'>WOTU TECHNOLOGIES PRIVATE LIMITED </td></tr>
            <tr> <td colspan='2' valign='middle' align='center' style='font-size: 20px;font-weight: bold; padding: 3px 0;margin-top:5px;margin-bottom:10px;padding-top:3px;padding-bottom:3px;'> DEBIT NOTE </td></tr>
            <tr>
                <td align='center' colspan='2'>
                    <table width='100%' cellspacing='0' style='border-collapse: collapse;border:1px solid'>
                        <tr>
                            <td style='vertical-align: top; padding: 10px;'> <table width='100%' style='font-size: 11px;'> <tr> <td style='font-weight: bold;padding-bottom:4px;width:40%;'>Debit Note No:</td><td style='padding-bottom:4px;'>@Model.DebitNoteNumber</td></tr><tr> <td style='font-weight: bold;padding-bottom:4px;width:40%;'>Purchase Order No:</td><td style='padding-bottom:4px;'>@Model.PurchaseOrderNumber</td></tr><tr> <td style='font-weight: bold;padding-bottom:4px;width:40%;'>GRN No:</td><td style='padding-bottom:4px;'>@Model.InvoiceNumber</td></tr><tr> <td style='font-weight: bold;padding-bottom:4px;width:40%;'>Debit Note Creation Date:</td><td style='padding-bottom:4px;'>@Model.DebitNoteCreationDateTime</td></tr><tr> <td style='font-weight: bold;padding-bottom:4px;width:40%;'>Status:</td><td style='padding-bottom:4px;'>@Model.Status</td></tr><tr> <td style='font-weight: bold;padding-bottom:4px;width:40%;'>GRN date:</td><td style='padding-bottom:4px;'>@Model.DeliveredDateTime.ToShortDateString()</td></tr></table> </td>
                            <td style='vertical-align: top; padding: 10px;width:45%;'>
                                <table width='100%' style='font-size: 11px;'>
                                    <tr> <td style='font-weight: bold;padding-bottom:4px;width:29%;'>Vendor:</td><td>@Model.VendorAccount</td></tr>
                                    <tr>
                                        <td style='font-weight: bold;padding-bottom:4px;width:29%;'>Vendor Address:</td>
                                        <td style='text-transform: uppercase;padding-bottom:4px;'>
                                            <strong>@Model.VendorAddressName</strong> @Model.VendorAddress,
                                            <br /> @Model.VendorLocality,
                                            <br /> @Model.VendorCity,
                                            @Model.VendorState @Model.VendorZip
                                        </td>
                                    </tr>
                                    <tr> <td style='font-weight: bold;padding-bottom:6px;width:25%'>Vendor GSTIN:</td><td style='padding-bottom:6px;'>@Model.VendorGstin</td></tr>
                                </table>
                            </td>
                        </tr>
                    </table>
                </td>
            </tr>
            <tr>
                <td align='center' colspan='2'>
                    <table width='100%' cellspacing='0' style='margin-top: -4px; font-size: 11px;'>
                        <thead style='text-align: left;'> <tr> <th style='border-right: 1px solid; border-bottom: 1px solid; border-left:1px solid;padding: 5px;width:6%;text-align:center;'>S.No.</th> <th style='border-right: 1px solid;width:11%; border-bottom: 1px solid; padding: 5px;text-align:center;'>Product</th> <th style='border-right: 1px solid;padding: 5px;width:9%;text-align:center;border-bottom: 1px solid;'>HSN Code</th> <th style='border-right: 1px solid;width:9%; border-bottom: 1px solid; padding: 5px;text-align:center;'>SKU</th> <th style='border-right: 1px solid; border-bottom: 1px solid; padding: 5px;width:10%;text-align:center;'>Qty. Ord.</th> <th style='border-right: 1px solid; border-bottom: 1px solid; padding: 5px;width:10%;text-align:center;'>Debit Qty.</th> <th style='border-right: 1px solid; border-bottom: 1px solid; padding: 5px;width:9%;text-align:center;'>Unit Price</th> <th style='border-right: 1px solid; border-bottom: 1px solid; padding: 5px;width:7%;text-align:center;'>UoM</th> <th style='border-right: 1px solid; border-bottom: 1px solid; padding: 5px;width:9%;text-align:center;'>Price</th> <th colspan='2' style='border-right: 1px solid; padding:0;width:30%;border-bottom: 1px solid;'> <table style='font-size: 11px;' cellspacing='0'> <tr> <th colspan='2' style='border-bottom: 1px solid;padding:5px;text-align:center;'>CGST</th> </tr><tr> <th style='border-right: 1px solid;padding:5px;width:25%;'>Rate(%)</th> <th style='padding:5px;width:25%;'>Amount</th> </tr></table> </th> <th colspan='2' style='border-right: 1px solid;padding:0;width:30%;border-bottom: 1px solid;'> <table style='font-size: 11px;' cellspacing='0'> <tr> <th colspan='2' style='border-bottom: 1px solid;padding:5px;text-align:center;'>SGST</th> </tr><tr> <th style='border-right: 1px solid;padding:5px;width:25%; '>Rate(%)</th> <th style='padding:5px;width:25%;'>Amount</th> </tr></table> </th> <th style='border-bottom: 1px solid; padding: 5px;border-right:1px solid;width:11%;text-align:center;'>Total</th> </tr></thead>
                        <tbody>
                            @foreach (var product in @Model.DebitNoteDetails)
                            {
                                <tr> <td style='padding: 5px;border-left:1px solid;width:7%;'>@product.SrNo</td><td style='padding: 5px;width:12%;border-left:1px solid;'>@product.ProductName</td><td style='padding: 5px;width:9%;border-left:1px solid;'>@product.HsnCode</td><td style='padding: 5px;width:9%;border-left:1px solid;'>@product.Sku</td><td style='padding: 5px;width:10%;border-left:1px solid;'>@product.QuantityOrdered</td><td style='padding: 5px;width:10%;border-left:1px solid;'>@product.DebitQuantity</td><td style='padding: 5px;width:12%;border-left:1px solid;'>@product.PricePerUnit</td><td style='padding: 5px;width:7%;border-left:1px solid;'>@product.ProductMeasurementUnit</td><td align='right' style='padding: 5px;width:12%;border-left:1px solid;'>@product.DebitAmount</td><td align='right' style='padding: 5px;width:12%;border-left:1px solid;'>@product.CgstRate</td><td align='right' style='padding: 5px;width:12%;border-left:1px solid;'>@product.CgstAmount</td><td align='right' style='padding: 5px;width:12%;border-left:1px solid;'>@product.SgstRate</td><td align='right' style='padding: 5px;width:12%;border-left:1px solid;'>@product.SgstAmount</td><td align='right' style='padding: 5px;border-left:1px solid;border-right:1px solid;width:9%;'>@product.DebitAmountWithGst</td></tr>
                            }

                            <tr> <td colspan='9' align='right' style='padding: 5px; font-weight: bold;border-top:1px solid;border-left:1px solid; '>@Model.TotalDebitAmountWithoutGst</td><td colspan='2' align='right' style='padding: 5px; font-weight: bold;border-top:1px solid '>@Model.TotalCgstAmount</td><td colspan='2' align='right' style='padding: 5px; font-weight: bold;border-top:1px solid '>@Model.TotalSgstAmount</td><td align='right' style='padding: 5px; font-weight: bold;border-right:1px solid;border-top:1px solid '>@Model.TotalDebitAmount</td></tr>
                            <tr>
                                <td align='center' colspan='14'>
                                    <table width='100% ' cellspacing='0 ' style='border-collapse: collapse;border:1px solid; margin-top: -4px; '>
                                        <tr> <td style='font-weight: bold;padding:10px;font-size: 13px;width:100%;padding-bottom:0;padding-left:20px;' colspan='2'> WOTU Details: </td></tr>
                                        <tr>
                                            <td style='width: 50%;vertical-align:top;padding:10px; '> <table width='100% ' style='font-size: 12px; '> <tr> <td style='font-weight: bold;padding-left:10px;width:16%; '>TIN:</td><td>@Model.TINNumber</td></tr><tr> <td style='font-weight: bold;padding-left:10px;width:16% '>PAN:</td><td>@Model.PANNumber</td></tr><tr> <td style='font-weight: bold;padding-left:10px;width:16% '>FSSAI:</td><td>@Model.FSSAINumber</td></tr><tr> <td style='font-weight: bold;padding-left:10px;width:16%'>GSTIN:</td><td style='padding-bottom:6px;'>@Model.WotuGSTIN</td></tr></table> </td>
                                            <td style='vertical-align: top; padding: 10px; '>
                                                <table width='100% ' style='font-size: 12px; '>
                                                    <tr> <td style='font-weight: bold; padding-bottom: 38px;'>Vendor's Signature :</td></tr>
                                                    <tr>
                                                        <td style='font-weight: bold;
'>
                                                            Place & Date :
                                                        </td>
                                                    </tr>
                                                </table>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td colspan='2 ' style='font-size:12px;
font-weight: bold;
padding-left:20px;
'>
                                                Declaration :
                                            </td>
                                        </tr>
                                        <tr>
                                            <td colspan='2 ' style='font-size:12px;
padding-left:20px;
padding-bottom:10px;
'>
                                                We declare that this Debit note shows the actual price of the goods described and that all particulars are true and correct.
                                            </td>
                                        </tr>
                                    </table>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </td>
            </tr>
            <tr>
                <td colspan='2'>
                    <table style='background: #F2F2F2;
font-size: 14px;
margin: 10px auto;
border: 0;
' cellspacing='0' align='center'>
                        <tr>
                            <td align='center' style='padding-top:12px;
padding-bottom:12px;
padding-right:6px;
padding-left:6px;
'>
                                WOTU Technologies Pvt Ltd. #83, 4th cross, 16th C Main, 4th block, Koramangala, Bangalore- 560034 <br /> www.wotu.in
                            </td>
                        </tr>
                    </table>
                </td>
            </tr>
        </table>

    </div>
</body>
</html>
